[93m 15 [m    sp = fsps.StellarPopulation(compute_vega_mags = [1m[96mFalse[m,
[93m 16 [m[32Czcontinuous = [95m1[m,
[93m 17 [m[32Csfh = [95m0[m,
[93m 18 [m[32Clogzsol = [95m0.0[m,
[93m 19 [m[32Cadd_dust_emission = [1m[96mFalse[m,
[93m 20 [m[32Cadd_neb_emission = -[95m1[m,)
[93m 21 
 22 [m    [93mreturn[m sp
[93m 23 
 24 
 25 
 26 
 27 [m[93mdef[m [1m[96mFSPS_SED_generator[m(param_names,
[93m 28 [m[23Cparam_grid,
[93m 29 [m[23CFSPS_initializer):
[93m 30 [m    [96m# This generates galaxy SEDs based on the Stellar Population code[m
[93m 31 [m    [96m# It calls a FSPS_initializer code, depending on the input[m
[93m 32 [m    [96m#           1. NO DUST?[m
[93m 33 [m    [96m#           2. NO IGM?[m
[93m 34 [m    [96m#           3. NO NEBULAE?[m
[93m 35 
 36 
 37 [m    [96m# Initialize the stellar population synthesis code[m
[93m 38 [m    sp = FSPS_initializer
[93m 39 
 40 [m    tage_grid, logzsol_grid = param_grid[:, [95m0[m], param_grid[:, [95m1[m]
[93m 41 
 42 [m    [93mfor[m age, logzsol [93min[m [1m[96mzip[m(tage_grid, logzsol_grid):
[93m 43 [m[8C[1m[96mprint[m (age, logzsol)
[93m 44 
 45 
 46 
 47 [m[96m#     wave, SED = sp.get_spectrum(tage = 0.001, peraa = True)[m
[93m 48 [m[96m#     print (SED)[m
[93m 49 [m[96m# [m
[93m 50 [m[96m#     sp.params['logzsol'] = 0.0[m
[93m 51 [m[96m# [m
[93m 52 [m[96m#     return wave, SED[m
[93m 53 
